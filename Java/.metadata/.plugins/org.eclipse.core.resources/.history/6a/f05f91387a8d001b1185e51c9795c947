package Exemplo;

public class Programa1 {

	public static void main(String[] args) {
		
		int valor1, valor2, soma;
		
		valor1 = 10;
		valor2 = 30;
		
		soma = valor1 + valor2;
		
		System.out.println("O valor 1 é: " + valor1);
		System.out.println("O valor 2 é: " + valor2);
		System.out.println("A soma dos valore é: " + soma);
		
		

	}

}
